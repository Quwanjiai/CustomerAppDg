<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="178dp"
        android:layout_marginTop="614dp"
        android:layout_marginEnd="178dp"
        android:layout_marginBottom="61dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.625"
        app:srcCompat="@drawable/ic_launcher_foreground" />

    <ImageButton
        android:id="@+id/rightButton"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="288dp"
        android:layout_marginTop="558dp"
        android:layout_marginEnd="68dp"
        android:layout_marginBottom="122dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:drawable/ic_media_ff" />

    <ImageButton
        android:id="@+id/leftButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="59dp"
        android:layout_marginTop="554dp"
        android:layout_marginEnd="163dp"
        android:layout_marginBottom="123dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/rightButton"
        app:layout_constraintHorizontal_bias="0.7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@android:drawable/ic_media_rew" />

    <TextView
        android:id="@+id/FNtextView"
        android:layout_width="102dp"
        android:layout_height="28dp"
        android:layout_marginStart="114dp"
        android:layout_marginEnd="239dp"
        android:layout_marginBottom="381dp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@id/blankLF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.659"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.501" />

    <Space
        android:id="@+id/blankLF"
        android:layout_width="303dp"
        android:layout_height="10dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/LNtextView"
        android:layout_width="102dp"
        android:layout_height="28dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="149dp"
        android:layout_marginEnd="98dp"
        android:layout_marginBottom="19dp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@id/blankLF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.909"
        app:layout_constraintStart_toEndOf="@+id/FNtextView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/phoneTextView"
        android:layout_width="231dp"
        android:layout_height="51dp"
        android:layout_marginStart="85dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="98dp"
        android:layout_marginBottom="14dp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@id/blankLF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FNtextView"
        app:layout_constraintVertical_bias="0.033" />

    <TextView
        android:id="@+id/adstextView"
        android:layout_width="231dp"
        android:layout_height="60dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="110dp"
        android:layout_marginEnd="99dp"
        android:layout_marginBottom="8dp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@id/blankLF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/FNtextView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/emtextView"
        android:layout_width="231dp"
        android:layout_height="60dp"
        android:layout_marginStart="78dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="95dp"
        android:layout_marginBottom="252dp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@id/blankLF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.857"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/FNtextView"
        app:layout_constraintVertical_bias="0.913" />

</androidx.constraintlayout.widget.ConstraintLayout>